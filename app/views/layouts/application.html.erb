<!DOCTYPE html>
<html>
  <head>
    <title>PortfolioWebsite</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

    <div class="container">
      <%= render "shared/nav" %>

      <%= flash[:alert] %>
      <%= flash[:notice] %>

      <%= yield %>

      <%= render "shared/footer" %>
    </div>
  </body>
</html>
